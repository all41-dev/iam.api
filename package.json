    "@types/validator": "13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cp-cli": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "npm-run-all": "^4.1.5",
    "request-promise": "^4.2.6",
