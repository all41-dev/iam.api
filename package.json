{
  "name": "@all41-dev/iam.api",
  "version": "1.3.1",
  "description": "Api serving user management operations and OAuth service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint src/** --fix",
    "build": "tsc && npm run copy:scripts",
